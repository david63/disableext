{
	"name": "david63/disableext",
	"type": "phpbb-extension",
	"description": "Adds an option to disable all phpBB extensions.",
	"version": "3.1.0-RC1",
	"time": "2019-12-29",
	"keywords": [
		"phpbb",
		"extension",
		"disable"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Developer"
		}
    ],
	"require": {
        "php": ">=7.1.0",
		"phpbb/phpbb": ">=3.3.0",
        "composer/installers": "~1.0"
    },
	"extra": {
        "display-name": "Disable all extensions",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/disable_all_extensions",
            "filename": "version_check"
        }
    }
}
